          rdfs:label "durata traccia"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haGenere
:haGenere rdf:type owl:DatatypeProperty ;
          rdfs:domain :Traccia ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf [ rdf:type rdf:List ;
                                   rdf:first "Classica" ;
                                   rdf:rest [ rdf:type rdf:List ;
                                              rdf:first "Indie" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "Jazz" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "Pop" ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "Rock" ;
                                                                               rdf:rest rdf:nil
                                                                             ]
                                                                  ]
                                                       ]
                                            ]
                                 ]
                     ] ;
          rdfs:comment "Genere di una traccia musicale"@it ;
          rdfs:label "haGenere"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haScala
:haScala rdf:type owl:DatatypeProperty ;
         rdfs:domain :Traccia ;
         rdfs:range [ rdf:type rdfs:Datatype ;
                      owl:oneOf [ rdf:type rdf:List ;
                                  rdf:first "Maggiore" ;
                                  rdf:rest [ rdf:type rdf:List ;
                                             rdf:first "Minore" ;
                                             rdf:rest rdf:nil
                                           ]
                                ]
                    ] ;
         rdfs:comment "Scala musicale di una traccia musicale"@it ;
         rdfs:label "haScala"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haSupporto
:haSupporto rdf:type owl:DatatypeProperty ;
            rdfs:domain :Release ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
