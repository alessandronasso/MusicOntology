#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#isRealizedBy
<http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#isRealizedBy> rdf:type owl:ObjectProperty ;
                                                                                       owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#realizes> ;
                                                                                       rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationObject> ;
                                                                                       rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationRealization> ;
                                                                                       rdfs:comment "A relation between an information realization and an information object, e.g. the paper copy of the Italian Constitution realizes the text of the Constitution." ;
                                                                                       rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl> ;
                                                                                       rdfs:label "is realized by"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#realizes
<http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#realizes> rdf:type owl:ObjectProperty ;
                                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationRealization> ;
                                                                                   rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationObject> ;
                                                                                   rdfs:comment "A relation between an information realization and an information object, e.g. the paper copy of the Italian Constitution realizes the text of the Constitution." ;
                                                                                   rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl> ;
                                                                                   rdfs:label "realizes"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#firstItemOf
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#firstItemOf> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> ;
                                                                    rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                    rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                                                                    rdfs:label "first item of" .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasFirstItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasFirstItem> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ;
                                                                     owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#firstItemOf> ;
                                                                     rdf:type owl:FunctionalProperty ;
                                                                     rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                                                                     rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                     rdfs:comment "first item - The link to the first item of the list" ;
                                                                     rdfs:label "has first item" .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasLastItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasLastItem> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ;
                                                                    rdf:type owl:FunctionalProperty ;
                                                                    rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                                                                    rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                    rdfs:comment "last item - The link to the last item of the list" ;
                                                                    rdfs:label "has last item" .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#lastItemOf
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#lastItemOf> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> ;
                                                                   owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasLastItem> ;
                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                   rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                                                                   rdfs:label "last item of" .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows> ;
                                                                 owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem> ;
                                                                 rdf:type owl:FunctionalProperty ;
                                                                 rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                 rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                 rdfs:comment "next item - The link to the next item in a list (ordered collection)" ;
                                                                 rdfs:label "next item" .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes> ;
                                                                     rdf:type owl:FunctionalProperty ;
                                                                     rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                     rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                                                     rdfs:comment "previous item - The link to the previous item in a list (ordered collection)" ;
                                                                     rdfs:label "previous item" .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_di
:album_di rdf:type owl:ObjectProperty ;
          owl:inverseOf :ha_album ;
          rdfs:domain :Album ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Gruppo
                                     :Solo
                                   )
                     ] ;
          rdfs:comment "Relazione che indica a quale artista appartiene un certo album"@it ;
          rdfs:label "album_di"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_ha_traccia
:album_ha_traccia rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ;
                  owl:inverseOf :traccia_di_album ;
                  rdfs:domain :Album ;
                  rdfs:range :Traccia ;
                  rdfs:comment "Relazione che indica quale traccia appartiene all'album"@it ;
                  rdfs:label "album_ha_traccia"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#artistaPartecipanteGruppo
:artistaPartecipanteGruppo rdf:type owl:ObjectProperty ;
                           rdfs:domain :RuoloComponenteNelGruppo ;
                           rdfs:range :Artista ;
                           rdfs:comment "Artista che partecipa ad un gruppo"@it ;
                           rdfs:label "artista che partecipa ad un gruppo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#canzone_realizzata_da
:canzone_realizzata_da rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#isRealizedBy> ;
                       owl:inverseOf :traccia_realizza ;
                       rdfs:domain :Canzone ;
                       rdfs:range :Traccia ;
                       rdfs:comment "Relazione che indica quale canzone realizza la traccia"@it ;
                       rdfs:label "canzone_realizzata_da"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#compostaDa
:compostaDa rdf:type owl:ObjectProperty ;
            rdfs:domain :Canzone ;
            rdfs:range :Compositore ;
            rdfs:comment "Relazione che lega la canzone con il suo compositore"@it ;
            rdfs:label "composta da"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#contiene
:contiene rdf:type owl:ObjectProperty ;
          owl:inverseOf :pubblicato_in ;
          rdfs:domain :Release ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Album
                                     :Traccia
                                   )
                     ] ;
          rdfs:comment "Relazione che indica quale album o traccia musicale (singolo) contiene una release"@it ;
          rdfs:label "contiene"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#featured
:featured rdf:type owl:ObjectProperty ;
          owl:inverseOf :ha_featuring ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Gruppo
                                      :Solo
                                    )
                      ] ;
          rdfs:range :Traccia ;
          rdfs:comment "Relazione che indica che un certo gruppo o solista ha partecipato come featuring ad una traccia musicale"@it ;
          rdfs:label "featured"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppoPartecipante
:gruppoPartecipante rdf:type owl:ObjectProperty ;
                    rdfs:domain :RuoloComponenteNelGruppo ;
                    rdfs:range :Gruppo ;
                    rdfs:comment "Gruppo del partecipante"@it ;
                    rdfs:label "gruppo partecipante"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppo_ha_membro
:gruppo_ha_membro rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                  owl:inverseOf :membro_di_gruppo ;
                  rdfs:domain :Gruppo ;
                  rdfs:range :Artista ;
                  owl:propertyChainAxiom ( [ owl:inverseOf :gruppoPartecipante
                                           ]
                                           :artistaPartecipanteGruppo
                                         ) ;
                  rdfs:comment "Relazione che indica quale artista fa parte del gruppo"@it ;
                  rdfs:label "gruppo_ha_membro"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haCollaborato
:haCollaborato rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Artista
                                           :Gruppo
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Artista
                                          :Gruppo
                                        )
                          ] ;
               owl:propertyChainAxiom ( :ha_featuring
                                        :performata_da
                                      ) ;
               rdfs:comment "Relazione che lega due artisti che hanno effettuato una collaborazione"@it ;
               rdfs:label "ha collaborato"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_album
:ha_album rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Gruppo
                                      :Solo
                                    )
                      ] ;
          rdfs:range :Album ;
          rdfs:comment "Relazione che indica che un certo album appartiene ad un certo artista"@it ;
          rdfs:label "ha_album"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_contrattato
:ha_contrattato rdf:type owl:ObjectProperty ;
                owl:inverseOf :ha_etichetta ;
                rdfs:domain :CasaDiscografica ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Gruppo
                                           :Solo
                                         )
                           ] ;
                rdfs:comment "Relazione che indica quali artisti ha sotto contratto la casa discografica"@it ;
                rdfs:label "ha_contrattato"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_dipendente
:ha_dipendente rdf:type owl:ObjectProperty ;
               rdfs:domain :CasaDiscografica ;
               rdfs:comment "Relazione che indica quale editore lavora per la casa discografica"@it ;
               rdfs:label "ha_dipendente"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_etichetta
:ha_etichetta rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Gruppo
                                          :Solo
                                        )
                          ] ;
              rdfs:range :CasaDiscografica ;
              rdfs:comment "Relazione che indica quale etichetta ha l'artista"@it ;
              rdfs:label "ha_etichetta"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_featuring
:ha_featuring rdf:type owl:ObjectProperty ;
              rdfs:domain :Traccia ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Gruppo
                                         :Solo
                                       )
                         ] ;
              rdfs:comment "Relazione che indica che una certa traccia ha un certo artista come ft."@it ;
              rdfs:label "ha_featuring"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_produttore
:ha_produttore rdf:type owl:ObjectProperty ;
               rdfs:domain :Produttore ;
               rdfs:range :Recording ;
               rdfs:comment "Relazione che indica quale produttore ha partecipato ad un recording"@it ;
               rdfs:label "ha produttore"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#membro_di_gruppo
:membro_di_gruppo rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                  rdfs:domain :Artista ;
                  rdfs:range :Gruppo ;
                  owl:propertyChainAxiom ( [ owl:inverseOf :artistaPartecipanteGruppo
                                           ]
                                           :gruppoPartecipante
                                         ) ;
                  rdfs:comment "Relazione che indica a quale gruppo appartiene un artista"@it ;
                  rdfs:label "membro_di_gruppo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#nuova_release_di
:nuova_release_di rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain :Release ;
                  rdfs:range :Release ;
                  rdfs:comment "Nuova versione di una release precedente"@it ;
                  rdfs:label "nuova_release_di"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performa
:performa rdf:type owl:ObjectProperty ;
          owl:inverseOf :performata_da ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Gruppo
                                      :Solo
                                    )
                      ] ;
          rdfs:range :Traccia ;
          owl:propertyChainAxiom ( :ha_album
                                   :album_ha_traccia
                                 ) ;
          rdfs:comment "Relazione che indica quale artista ha realizzato la traccia musicale"@it ;
          rdfs:label "performa"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performata_da
:performata_da rdf:type owl:ObjectProperty ;
               rdfs:domain :Traccia ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Gruppo
                                          :Solo
                                        )
                          ] ;
               rdfs:comment "Relazione che indica quale artista ha realizzato una traccia musicale"@it ;
               rdfs:label "performata_da"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#pubblica
:pubblica rdf:type owl:ObjectProperty ;
          owl:inverseOf :pubblicata_da ;
          rdfs:domain :CasaDiscografica ;
          rdfs:range :Release ;
          rdfs:comment "Relazione che indica quale casa discografica ha pubblicato una release"@it ;
          rdfs:label "pubblica"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#pubblicata_da
:pubblicata_da rdf:type owl:ObjectProperty ;
               rdfs:domain :Release ;
               rdfs:range :CasaDiscografica ;
               rdfs:comment "Relazione che indica quale casa discografica ha pubblicato una certa release"@it ;
               rdfs:label "pubblicata_da"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#pubblicato_in
:pubblicato_in rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Album
                                           :Traccia
                                         )
                           ] ;
               rdfs:range :Release ;
               rdfs:comment "Relazione che indica in quale release è stato pubblicato un album o una traccia musicale"@it ;
               rdfs:label "pubblicato_in"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#registra
:registra rdf:type owl:ObjectProperty ;
          owl:inverseOf :registrata_in ;
          rdf:type owl:InverseFunctionalProperty ;
          rdfs:domain :Recording ;
          rdfs:range :Traccia ;
          rdfs:comment "Relazione che indica quale traccia musicale è stata reigstrata in un recording"@it ;
          rdfs:label "registra"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#registrata_in
:registrata_in rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Traccia ;
               rdfs:range :Recording ;
               rdfs:comment "Relazione che indica in quale recording è stata registrata una traccia"@it ;
               rdfs:label "registrata in"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ruoloArtistaPartecipanteGruppo
:ruoloArtistaPartecipanteGruppo rdf:type owl:ObjectProperty ;
                                rdfs:domain :RuoloComponenteNelGruppo ;
                                rdfs:range :Musicista ;
                                rdfs:comment "Ruolo dell'artista partecipante al gruppo" ;
                                rdfs:label "ruolo artista che partecipa ad un gruppo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#scrittaDa
:scrittaDa rdf:type owl:ObjectProperty ;
           rdfs:domain :Canzone ;
           rdfs:range :Scrittore ;
           rdfs:comment "Relazione che lega la canzone con il suo scrittore"@it ;
           rdfs:label "scritta da"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#traccia_di_album
:traccia_di_album rdf:type owl:ObjectProperty ;
