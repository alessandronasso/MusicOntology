:Album rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
       rdfs:comment "Un album discografico è un supporto discografico (in vinile o LP, CD, musicassetta, Stereo8, download digitale o altro formato) di lunga durata, contenente una raccolta di brani musicali di uno stesso artista o di più artisti differenti (compilation)."@it ;
       rdfs:label "Album"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista
:Artista rdf:type owl:Class ;
         rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
         owl:disjointWith <http://xmlns.com/foaf/0.1/Group> ;
         rdfs:comment "Persona della cui attività si esprime nel campo della musica"@it ;
         rdfs:label "Artista"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#CD
:CD rdf:type owl:Class ;
    owl:equivalentClass [ rdf:type owl:Restriction ;
                          owl:onProperty :haSupporto ;
                          owl:hasValue "CD"
                        ] ;
    rdfs:subClassOf :Supporto ;
    rdfs:comment "Il compact disc è un tipo standardizzato di disco ottico utilizzato in vari ambiti per la memorizzazione di informazioni in formato digitale."@it ;
    rdfs:label "CD"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Calma
:Calma rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :haBPM ;
                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                  owl:onDatatype xsd:integer ;
                                                  owl:withRestrictions ( [ xsd:maxInclusive 80
                                                                         ]
                                                                       )
                                                ]
                           ] ;
       rdfs:subClassOf :Umore ;
       owl:disjointWith :Energetico ;
       rdfs:comment "Stato d'animo calmo"@it ;
       rdfs:label "Calma"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Canzone
:Canzone rdf:type owl:Class ;
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationObject> ;
         rdfs:comment "Composizione vocale e scritta"@it ;
         rdfs:label "Canzone"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#CasaDiscografica
:CasaDiscografica rdf:type owl:Class ;
                  rdfs:comment "Azienda specializzata in produzione e distribuzione di musica"@it ;
                  rdfs:label "Casa discografica"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Classica
:Classica rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :haGenere ;
                                owl:hasValue "Classica"
                              ] ;
          rdfs:subClassOf :Genere ;
          rdfs:comment "Col termine musica classica ci si riferisce alla musica colta, sacra e profana, composta o avente radici nel contesto della cultura occidentale."@it ;
          rdfs:label "Classica"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Compositore
:Compositore rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf :compostaDa
                                                  ] ;
                                   owl:someValuesFrom <http://xmlns.com/foaf/0.1/Person>
                                 ] ;
             rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
             rdfs:comment "Coloro che hanno contribuito alla tradizione della musica classica occidentale attraverso la creazione di opere espresse in notazione musicale scritta."@it ;
             rdfs:label "Compositore"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Digitale
:Digitale rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :haSupporto ;
                                owl:hasValue "Digitale"
                              ] ;
          rdfs:subClassOf :Supporto ;
          rdfs:comment "Con il termine musica digitale si intende una qualsiasi composizione musicale convertita in segnale digitale."@it ;
          rdfs:label "Digitale"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Energetico
:Energetico rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :haBPM ;
                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:onDatatype xsd:integer ;
                                                       owl:withRestrictions ( [ xsd:minExclusive 80
                                                                              ]
                                                                            )
                                                     ]
                                ] ;
            rdfs:subClassOf :Umore ;
            rdfs:comment "Stato d'animo energetico"@it ;
            rdfs:label "Energetico"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Felice
:Felice rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :haScala ;
                              owl:hasValue "Maggiore"
                            ] ;
        rdfs:subClassOf :Umore ;
        owl:disjointWith :Triste ;
        rdfs:comment "Stato d'animo felice"@it ;
        rdfs:label "Felice"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Genere
:Genere rdf:type owl:Class ;
        rdfs:comment "Categoria che identifica brani musicali"@it ;
        rdfs:label "Genere musicale"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo
:Gruppo rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :gruppo_ha_membro ;
                              owl:someValuesFrom :Artista
                            ] ;
        rdfs:subClassOf <http://xmlns.com/foaf/0.1/Group> ;
        owl:disjointWith :Solo ;
        rdfs:comment "Sodalizio artistico composto da persone che suonano stabilmente musica insieme"@it ;
        rdfs:label "Gruppo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Indie
:Indie rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :haGenere ;
                             owl:hasValue "Indie"
                           ] ;
       rdfs:subClassOf :Genere ;
       rdfs:comment "L'indie è un genere musicale di alternative rock nato nel Regno Unito e negli Stati Uniti alla fine degli anni ottanta e diffusosi, soprattutto a partire dagli anni 2000"@it ;
       rdfs:label "Indie"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Jazz
:Jazz rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :haGenere ;
                            owl:hasValue "Jazz"
                          ] ;
      rdfs:subClassOf :Genere ;
      rdfs:comment "Il jazz è un genere musicale nato agli inizi del XX secolo come evoluzione di forme musicali già utilizzate dagli schiavi afroamericani."@it ;
      rdfs:label "Jazz"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Musicista
:Musicista rdf:type owl:Class ;
           rdfs:subClassOf :Ruolo ;
           rdfs:comment "Persona che svolge e ha talento per una determinata attività in musica"@it ;
           rdfs:label "Musicista"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Pop
:Pop rdf:type owl:Class ;
     owl:equivalentClass [ rdf:type owl:Restriction ;
                           owl:onProperty :haGenere ;
                           owl:hasValue "Pop"
                         ] ;
     rdfs:subClassOf :Genere ;
     rdfs:comment "La musica pop, traduzione del termine inglese pop music, è un genere musicale appartenente all'insieme della popular music, che trova origine, nella sua forma moderna, come derivazione del rock and roll."@it ;
     rdfs:label "Pop"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Produttore
:Produttore rdf:type owl:Class ;
            rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
            rdfs:comment "Persona che si occupa della supervisione delle sessioni in studio di registrazione"@it ;
            rdfs:label "Produttore"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Recording
:Recording rdf:type owl:Class ;
           rdfs:comment "La registrazione sonora è un processo elettrico, digitale o meccanico che consente di acquisire onde sonore, come ad esempio la voce umana, il canto, la musica strumentale o effetti sonori in genere."@it ;
           rdfs:label "Recording"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Release
:Release rdf:type owl:Class ;
         rdfs:comment "In informatica, nell'ambito dello sviluppo software, la release (sostantivo mutuato dall'inglese che tradotto letteralmente significa \"emissione\", \"uscita\" o \"distribuzione\", spesso erroneamente tradotto con \"rilascio\") è una specifica versione di un software resa disponibile ai suoi utenti finali"@it ;
         rdfs:label "Release"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Rock
:Rock rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :haGenere ;
                            owl:hasValue "Rock"
                          ] ;
      rdfs:subClassOf :Genere ;
      rdfs:comment "Il rock, o musica rock, è un genere della popular music sviluppatosi negli Stati Uniti e nel Regno Unito nel corso degli anni cinquanta e sessanta del Novecento."@it ;
      rdfs:label "Rock"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Ruolo
:Ruolo rdf:type owl:Class ;
       rdfs:comment "Ruolo ricoperto da un agente"@it ;
       rdfs:label "Ruolo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#RuoloComponenteNelGruppo
:RuoloComponenteNelGruppo rdf:type owl:Class ;
                          rdfs:comment "Una situazione che rappresenta il ruolo (o i ruoili) di uno specifico artista parteicpante ad un gruppo musicale"@it ;
                          rdfs:label "Ruolo dell'artista nel gruppo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Scrittore
:Scrittore rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty [ owl:inverseOf :scrittaDa
                                                ] ;
                                 owl:someValuesFrom <http://xmlns.com/foaf/0.1/Person>
                               ] ;
           rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
           rdfs:comment "Figura incaricata della produzione del testo musicale di una canzone."@it ;
           rdfs:label "Scrittore"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#SolistaItaliano
:SolistaItaliano rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Solo
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :nazione ;
                                                              owl:hasValue "Italiana"
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Solo ;
                 rdfs:comment "Persona di origini italiane che suona da solo in una composizione musicale"@it ;
                 rdfs:label "Solista Italiano"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo
:Solo rdf:type owl:Class ;
      rdfs:subClassOf :Artista ;
      rdfs:comment "Persona che suona da solo in una composizione musicale"@it ;
      rdfs:label "Solo"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Strumentale
:Strumentale rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :Requiem
                                             )
                                 ] ;
             rdfs:subClassOf :Canzone ;
             rdfs:comment "La musica strumentale è la musica che viene eseguita esclusivamente mediante l'impiego di strumenti musicali, senza l'utilizzo della parte vocale. I brani in cui è assente la musica strumentale, eseguiti con il solo utilizzo del canto individuale o corale, vengono detti 'a cappella'."@it ;
             rdfs:label "Strumentale"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Supporto
:Supporto rdf:type owl:Class ;
          rdfs:comment "Dispositivo di memorizzazione per tracce"@it ;
          rdfs:label "Supporto"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia
:Traccia rdf:type owl:Class ;
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#InformationRealization> ,
                         <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
         rdfs:comment "Brano musicale"@it ;
         rdfs:label "Traccia musicale"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Triste
:Triste rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :haScala ;
                              owl:hasValue "Minore"
                            ] ;
        rdfs:subClassOf :Umore ;
        rdfs:comment "Stato d'animo triste"@it ;
        rdfs:label "Triste"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Umore
:Umore rdf:type owl:Class ;
       rdfs:comment "Stato d'animo generato dalla traccia"@it ;
       rdfs:label "Umore"@it .


###  http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Vinile
:Vinile rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :haSupporto ;
                              owl:hasValue "Vinile"
                            ] ;
        rdfs:subClassOf :Supporto ;
        rdfs:comment "Il disco in vinile (noto anche come microsolco), è un supporto per la memorizzazione analogica di segnali sonori. È stato ufficialmente introdotto nel 1948 dalla Columbia records negli Stati Uniti d'America come evoluzione del precedente disco a 78 giri, dalle simili caratteristiche, realizzato in gommalacca."@it ;
        rdfs:label "Vinile"@it .


