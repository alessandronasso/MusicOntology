@prefix : <http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology> .

<http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology> rdf:type owl:Ontology ;
                                                                     owl:imports <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl> ,
                                                                                 <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ,
                                                                                 <http://xmlns.com/foaf/0.1/> ;
                                                                     <http://purl.org/dc/elements/1.1/creator> "Alessandro Nasso" ,
                                                                                                               "Bruno Lopardo" ;
                                                                     <http://purl.org/dc/elements/1.1/date> "11/02/2020"^^xsd:date ;
                                                                     <http://purl.org/dc/elements/1.1/description> "Ontologia si propone di modellare il dominio relativo all'industria musicale, pernettendo un'analisi delle relazioni tra prodotti musicali come release, album, tracce e le persone che vi hanno lavorato, quali musticsti, prodotuttori ed editori." ;
                                                                     <http://purl.org/dc/elements/1.1/title> "A music ontology" ;
                                                                     <http://purl.org/vocab/vann/preferredNamespacePrefix> "tmo" ;
                                                                     <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology"^^xsd:anyURI ;
                                                                     rdfs:label "Music Ontology" .

