    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_di -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_di">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_album"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Album"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione che indica a quale artista appartiene un certo album</rdfs:comment>
        <rdfs:label xml:lang="it">album_di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_ha_traccia -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_ha_traccia">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#traccia_di_album"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Album"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
        <rdfs:comment xml:lang="it">Relazione che indica quale traccia appartiene all&apos;album</rdfs:comment>
        <rdfs:label xml:lang="it">album_ha_traccia</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#artistaPartecipanteGruppo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#artistaPartecipanteGruppo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#RuoloComponenteNelGruppo"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista"/>
        <rdfs:comment xml:lang="it">Artista che partecipa ad un gruppo</rdfs:comment>
        <rdfs:label xml:lang="it">artista che partecipa ad un gruppo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#canzone_realizzata_da -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#canzone_realizzata_da">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#isRealizedBy"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#traccia_realizza"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Canzone"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
        <rdfs:comment xml:lang="it">Relazione che indica quale canzone realizza la traccia</rdfs:comment>
        <rdfs:label xml:lang="it">canzone_realizzata_da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#compostaDa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#compostaDa">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Canzone"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Compositore"/>
        <rdfs:comment xml:lang="it">Relazione che lega la canzone con il suo compositore</rdfs:comment>
        <rdfs:label xml:lang="it">composta da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#contiene -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#contiene">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#pubblicato_in"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Release"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Album"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione che indica quale album o traccia musicale (singolo) contiene una release</rdfs:comment>
        <rdfs:label xml:lang="it">contiene</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#featured -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#featured">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_featuring"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
        <rdfs:comment xml:lang="it">Relazione che indica che un certo gruppo o solista ha partecipato come featuring ad una traccia musicale</rdfs:comment>
        <rdfs:label xml:lang="it">featured</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppoPartecipante -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppoPartecipante">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#RuoloComponenteNelGruppo"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
        <rdfs:comment xml:lang="it">Gruppo del partecipante</rdfs:comment>
        <rdfs:label xml:lang="it">gruppo partecipante</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppo_ha_membro -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppo_ha_membro">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#membro_di_gruppo"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppoPartecipante"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#artistaPartecipanteGruppo"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Relazione che indica quale artista fa parte del gruppo</rdfs:comment>
        <rdfs:label xml:lang="it">gruppo_ha_membro</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haCollaborato -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#haCollaborato">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_featuring"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performata_da"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Relazione che lega due artisti che hanno effettuato una collaborazione</rdfs:comment>
        <rdfs:label xml:lang="it">ha collaborato</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_album -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_album">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Album"/>
        <rdfs:comment xml:lang="it">Relazione che indica che un certo album appartiene ad un certo artista</rdfs:comment>
        <rdfs:label xml:lang="it">ha_album</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_contrattato -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_contrattato">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_etichetta"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#CasaDiscografica"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione che indica quali artisti ha sotto contratto la casa discografica</rdfs:comment>
        <rdfs:label xml:lang="it">ha_contrattato</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_dipendente -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_dipendente">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#CasaDiscografica"/>
        <rdfs:comment xml:lang="it">Relazione che indica quale editore lavora per la casa discografica</rdfs:comment>
        <rdfs:label xml:lang="it">ha_dipendente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_etichetta -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_etichetta">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#CasaDiscografica"/>
        <rdfs:comment xml:lang="it">Relazione che indica quale etichetta ha l&apos;artista</rdfs:comment>
        <rdfs:label xml:lang="it">ha_etichetta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_featuring -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_featuring">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione che indica che una certa traccia ha un certo artista come ft.</rdfs:comment>
        <rdfs:label xml:lang="it">ha_featuring</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_produttore -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_produttore">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Recording"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Produttore"/>
        <rdfs:comment xml:lang="it">Relazione che indica quale produttore ha partecipato ad un recording</rdfs:comment>
        <rdfs:label xml:lang="it">ha produttore</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#membro_di_gruppo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#membro_di_gruppo">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Artista"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#artistaPartecipanteGruppo"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#gruppoPartecipante"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Relazione che indica a quale gruppo appartiene un artista</rdfs:comment>
        <rdfs:label xml:lang="it">membro_di_gruppo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#nuova_release_di -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#nuova_release_di">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Release"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Release"/>
        <rdfs:comment xml:lang="it">Nuova versione di una release precedente</rdfs:comment>
        <rdfs:label xml:lang="it">nuova_release_di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performa">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#performata_da"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Gruppo"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Solo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#Traccia"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#ha_album"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#album_ha_traccia"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Relazione che indica quale artista ha realizzato la traccia musicale</rdfs:comment>
        <rdfs:label xml:lang="it">performa</rdfs:label>
    </owl:ObjectProperty>
    


