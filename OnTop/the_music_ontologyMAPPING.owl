<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology"
     xml:base="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
