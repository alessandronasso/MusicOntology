     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:obda="https://w3id.org/obda/vocabulary#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:vann1="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#vann:"
     xmlns:music_ontology="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology#"
     xmlns:collectionentity="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#"
     xmlns:informationrealization="http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alessandro Nasso</dc:creator>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bruno Lopardo</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">11/02/2020</dc:date>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ontologia si propone di modellare il dominio relativo all&apos;industria musicale, pernettendo un&apos;analisi delle relazioni tra prodotti musicali come release, album, tracce e le persone che vi hanno lavorato, quali musticsti, prodotuttori ed editori.</dc:description>
        <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A music ontology</dc:title>
        <vann:preferredNamespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tmo</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.semanticweb.org/bruno/ontologies/2020/1/music_ontology</vann:preferredNamespaceUri>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Music Ontology</rdfs:label>
    </owl:Ontology>
    


